{
  "name": "guarda-meu-lanche-server",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "yarn  nodemon ./app.js",
    "knex:migrate": "knex --knexfile ./knex/knexfile.js migrate:latest --env production",
    "knex:rollback": "knex  --env-file=.env --knexfile ./knex/knexfile.js migrate:rollback --env production",
    "knex:reset": "knex --knexfile ./knex/knexfile.js migrate:rollback --env production && knex --knexfile ./knex/knexfile.js migrate:latest --env production",
    "": "knex --knexfile ./knex/knexfile.js migrate:make a"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "An api",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "sqlite3": "^5.1.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "sequelize-cli": "^6.6.2"
  }
}
